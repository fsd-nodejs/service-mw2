{
  "files.exclude": {
    "logs": true,
    "run": true,
    "package-lock.json": false,
    "node_modules": false,
    "coverage": true,
    "dist": false
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "terminal.integrated.autoReplies": {
    "Terminate batch job (Y/N)?": "Y\r",
    "终止批处理操作吗(Y/N)?": "Y\r",
  },
}